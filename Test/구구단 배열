using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace test_03
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[] dan = new int[72];
            int cnt = 0;
            for (int i = 2; i < 10; i++)
            {
                for (int j = 1; j < 10; j++)
                {
                    dan[cnt] = i * j;
                    Console.WriteLine(dan[cnt] + "\t");
                    cnt += 1;
                }
            }
            Console.WriteLine();
        }
    }
}
